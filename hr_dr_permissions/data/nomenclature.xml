<odoo>
    <data noupdate="1">
        <!--hr.nomenclature-->
        <record model="hr.nomenclature" id="NomenclaturePLCS1">
            <field name="name">DESCONTAR DE VACACIONES</field>
            <field name="acronym">DDV</field>
            <field name="data_type">boolean</field>
            <field name="description">Se utiliza para definir si el tiempo ocupado en un Permiso, Licencia o Comisión de Servicio va a ser descontado de las vacaciones o no.
            </field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS2">
            <field name="name">TIEMPO MÁXIMO PERMITIDO EN MINUTOS</field>
            <field name="acronym">TMPEMI</field>
            <field name="data_type">integer</field>
            <field name="description">Se utiliza para delimitar el tiempo máximo permitido (En minutos) para un Tipo de Permiso, Licencia o Comisión de Servicio.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS3">
            <field name="name">TIEMPO MÁXIMO PERMITIDO EN HORAS</field>
            <field name="acronym">TMPEH</field>
            <field name="data_type">integer</field>
            <field name="description">Se utiliza para delimitar el tiempo máximo permitido (En horas) para un Tipo de Permiso, Licencia o Comisión de Servicio.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS4">
            <field name="name">TIEMPO MÁXIMO PERMITIDO EN DÍAS</field>
            <field name="acronym">TMPED</field>
            <field name="data_type">integer</field>
            <field name="description">Se utiliza para delimitar el tiempo máximo permitido (En días) para un Tipo de Permiso, Licencia o Comisión de Servicio.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS5">
            <field name="name">TIEMPO MÁXIMO PERMITIDO EN MESES</field>
            <field name="acronym">TMPEME</field>
            <field name="data_type">integer</field>
            <field name="description">Se utiliza para delimitar el tiempo máximo permitido (En meses) para un Tipo de Permiso, Licencia o Comisión de Servicio.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS6">
            <field name="name">TIEMPO MÁXIMO PERMITIDO EN AÑOS</field>
            <field name="acronym">TMPEA</field>
            <field name="data_type">integer</field>
            <field name="description">Se utiliza para delimitar el tiempo máximo permitido (En años) para un Tipo de Permiso, Licencia o Comisión de Servicio.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS7">
            <field name="name">REQUIERE ADJUNTO</field>
            <field name="acronym">RA</field>
            <field name="data_type">boolean</field>
            <field name="description">Se utiliza para definir si una solicitud de Permiso, Licencia o Comisión de Servicio requiere adjunto.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS8">
            <field name="name">TIEMPO TRABAJADO</field>
            <field name="acronym">TT</field>
            <field name="data_type">boolean</field>
            <field name="description">Se utiliza para definir si un Tipo de Permiso, Licencia o Comisión de Servicio se considera como tiempo trabajado para el colaborador.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS9">
            <field name="name">VISIBLE PARA COLABORADORES</field>
            <field name="acronym">VPC</field>
            <field name="data_type">boolean</field>
            <field name="description">Se utiliza para definir si un Tipo de Permiso, Licencia o Comisión de Servicio puede ser solicitado por el colaborador o solo es ingresado desde RRHH.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS10">
            <field name="name">ACUMULA VACACIONES</field>
            <field name="acronym">ACV</field>
            <field name="data_type">boolean</field>
            <field name="description">Se utiliza para definir si durante el tiempo de Permiso, Licencia o Comisión de Servicio el colaborador continúa acumulando vacaciones.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS11">
            <field name="name">TIEMPO MÁXIMO PERMITIDO DESPUÉS DE LA INCORPORACIÓN PARA PRESENTAR JUSTIFICACIÓN (EN HORAS)</field>
            <field name="acronym">TMPDDLIPPJ</field>
            <field name="data_type">integer</field>
            <field name="description">Se utiliza para definir la cantidad de horas(Después de la incorporación) que el colaborador tiene para presentar la justificación de un Permiso, Licencia o Comisión de Servicio.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS12">
            <field name="name">ANTIGÜEDAD EN MESES PARA REALIZAR UNA SOLICITUD</field>
            <field name="acronym">AEMPRUS</field>
            <field name="data_type">integer</field>
            <field name="description">Se utiliza para definir la antigüedad en la empresa (En meses) que debe tener un colaborador para poder solicitar un Permiso, Licencia o Comisión de Servicio.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS13">
            <field name="name">ANTIGÜEDAD EN AÑOS PARA REALIZAR UNA SOLICITUD</field>
            <field name="acronym">AEAPRUS</field>
            <field name="data_type">integer</field>
            <field name="description">Se utiliza para definir la antigüedad en la empresa (En años) que debe tener un colaborador para poder solicitar un Permiso, Licencia o Comisión de Servicio.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS14">
            <field name="name">ES POSIBLE REALIZAR UNA SOLICITUD EN EL PASADO</field>
            <field name="acronym">EPRUSEEP</field>
            <field name="data_type">boolean</field>
            <field name="description">Se utiliza para definir si es posible realizar una solicitud de Permiso, Licencia o Comisión de Servicio en el pasado.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS15">
            <field name="name">DÍA A DÍA</field>
            <field name="acronym">DD</field>
            <field name="data_type">boolean</field>
            <field name="description">Se utiliza para definir si un Tipo de Permiso, Licencia o Comisión de Servicio se ingresa día a día en las horas solicitadas o se ingresa como un periodo de tiempo completo desde la fecha y hora inicial hasta la fecha y hora final.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS16">
            <field name="name">CANTIDAD DE HORAS DE ANTICIPACIÓN PARA LA SOLICITUD</field>
            <field name="acronym">CDHDAPLS</field>
            <field name="data_type">integer</field>
            <field name="description">Se utiliza para definir la cantidad de horas de anticipación con que se debe solicitar un Permiso, Licencia o Comisión de Servicio.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <record model="hr.nomenclature" id="NomenclaturePLCS17">
            <field name="name">RECUPERAR TIEMPO OCUPADO EN EL PERMISO</field>
            <field name="acronym">RTOEEP</field>
            <field name="data_type">boolean</field>
            <field name="description">Se utiliza para definir si el tiempo ocupado en un Permiso, Licencia o Comisión de Servicio es recuperable o no.</field>
            <field name="active">True</field>
            <field name="module_id" eval="ref('base.module_hr_dr_permissions')"/>
        </record>
        <!--hr.nomenclature-->
    </data>
</odoo>